# If .bash_profile exists, bash doesn't read .profile by default. Make sure it does
if [[ -f ~/.profile ]]; then
  . ~/.profile
fi

# If .bashrc exists, get the aliases and functions
if [[ -f ~/.bashrc ]]; then
  . ~/.bashrc
fi

if [ -f ~/.git-completion.bash ]; then
    . ~/.git-completion.bash
fi

